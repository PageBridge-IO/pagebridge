{
  "name": "@gsc-sanity/sanity-plugin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.js",
      "default": "./dist/schemas/index.js"
    }
  },
  "scripts": {
    "build": "pnpm exec tsc",
    "dev": "pnpm exec tsc --watch",
    "check-types": "pnpm exec tsc --noEmit",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "sanity": "^5.0.0",
    "@sanity/ui": "^3.0.0",
    "@sanity/icons": "^3.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.18"
  },
  "devDependencies": {
    "@gsc-sanity/eslint-config": "workspace:^",
    "@gsc-sanity/typescript-config": "workspace:^",
    "sanity": "^5.7.0",
    "@sanity/ui": "^3.1.11",
    "@sanity/icons": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3"
  }
}
